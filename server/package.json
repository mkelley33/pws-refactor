{
  "name": "pws-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "engines": {
    "node": "18.16.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc -b -v",
    "clean": "rimraf ./src/**/bin ./bin ./src/**/tsconfig.tsbuildinfo tsconfig.tsbuildinfo",
    "rebuild": "npm run clean && npm run build",
    "buildw": "tsc -b -v -w --preserveWatchOutput -watch --extendedDiagnostics",
    "eslint": "./node_modules/.bin/eslint \"./src/**/**.ts\"",
    "node": "set TS_NODE_FILES=true & node --inspect --loader ts-node/esm src/index.ts",
    "tsc-watch": "./node_modules/.bin/tsc-watch -b -v -i -w --noclear --compiler \"./node_modules/.bin/tsc\" --onSuccess \"node --inspect --es-module-specifier-resolution=node ./bin/index.js\"",
    "test": "NODE_ENV=test mocha --check-leaks -r tsconfig-paths/register -r ts-node/register \"tests/**/*.test.ts\"",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "author": "Michaux Kelley",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.1",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "mongoose": "^7.3.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "sharp": "^0.32.1",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.38",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.40.2",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/method-override": "^0.0.32",
    "@types/mocha": "^10.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/nodemailer": "^6.4.8",
    "@types/request": "^2.48.8",
    "@types/shelljs": "^0.8.12",
    "@types/sinon": "^10.0.15",
    "@types/swagger-jsdoc": "^6.0.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.43.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
